{
  "name": "base1",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "typedoc": "typedoc --out ./docs/ ./src",
    "compodoc": "npx compodoc -p tsconfig.app.json",
    "postinstall": "node patch.js",
    "build-platform-debug": "ng build --base-href ./ && gulp",
    "build-platform-production": "ng build --prod --base-href ./ && gulp",
    "build-platform-server": "ng build --configuration=server --base-href ./ && gulp",
    "build-electron-production": "ng build --configuration=electron --base-href ./ && gulp",
    "build-electron-package-mac": "ng build --configuration=electron --base-href ./ && gulp && electron-builder --mac --x64",
    "build-electron-package-win": "ng build --configuration=electron --base-href ./ && gulp && electron-builder --win --ia32",
    "start-platform": "node ./app.js",
    "start-electron": "electron ."
  },
  "private": true,
  "build": {
    "appId": "com.seventhcode.base1",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "portable"
    },
    "directories": {
      "output": "dmg"
    }
  },
